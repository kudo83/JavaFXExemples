<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="medias.MediaSController">
   <children>
      <MediaView fx:id="mediaViewer" fitHeight="360.0" fitWidth="640.0" layoutY="-1.0" />
      <Button layoutX="217.0" layoutY="361.0" mnemonicParsing="false" onAction="#onChooseMedia" text="Choose Media" />
      <Button layoutX="319.0" layoutY="362.0" mnemonicParsing="false" onAction="#onPlay">
         <graphic>
            <FontAwesomeIconView glyphName="PLAY" />
         </graphic>
      </Button>
      <Button layoutX="358.0" layoutY="362.0" mnemonicParsing="false" onAction="#onPause">
         <graphic>
            <FontAwesomeIconView glyphName="PAUSE" />
         </graphic>
      </Button>
      <Button layoutX="397.0" layoutY="362.0" mnemonicParsing="false" onAction="#onStop">
         <graphic>
            <FontAwesomeIconView glyphName="STOP" />
         </graphic>
      </Button>
   </children>
</AnchorPane>
